{
  "name": "gatsby-site",
  "private": true,
  "description": "Default Gatsby Starter customized for the usage of TypeScript, TDD approach and linting experience which encourages to follow the best coding practices.",
  "version": "0.1.0",
  "author": "Tomasz Grande <grande.tomasz@gmail.com>",
  "keywords": [
    "gatsby",
    "jest",
    "react",
    "styled-components",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/grande-tomasz/gatsby-custom-project-starter"
  },
  "bugs": {
    "url": "https://github.com/grande-tomasz/gatsby-custom-project-starter/issues"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.11.1",
    "gatsby": "^2.24.4",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-google-tagmanager": "^2.3.11",
    "gatsby-plugin-manifest": "^2.4.26",
    "gatsby-plugin-offline": "^3.2.26",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.30",
    "gatsby-plugin-styled-components": "^3.3.10",
    "gatsby-plugin-typescript": "^2.4.18",
    "gatsby-source-filesystem": "^2.3.27",
    "gatsby-transformer-sharp": "^2.5.13",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/react-test-renderer": "^16.9.3",
    "@types/styled-components": "^5.1.2",
    "@types/testing-library__jest-dom": "^5.9.2",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-jest": "^26.3.0",
    "babel-preset-gatsby": "^0.5.8",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-plugin-testing-library": "^3.7.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-styled-components": "^7.0.3",
    "prettier": "^2.1.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "build": "npm run gatsby:clean && npm run gatsby:build && npm run gatsby:serve",
    "clean": "npm run gatsby:clean && rimraf node_modules && echo 'Node modules have been deleted.'",
    "eslint:check": "eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "eslint:fix": "npm run eslint:check -- --fix",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "gatsby:build": "gatsby build",
    "gatsby:clean": "gatsby clean",
    "gatsby:develop": "gatsby develop",
    "gatsby:serve": "gatsby serve",
    "pre-commit": "npm run format && npm run eslint:fix && npm test",
    "pre-push": "npm run eslint:check && npm run stylelint:check && npm test",
    "project:new": "rimraf src/components/* src/images/* src/pages/* src/templates/* tests/* && echo 'Sample files have been deleted.'",
    "start": "npm run gatsby:develop",
    "stylelint:check": "stylelint ./src/**/*.tsx --allow-empty-input",
    "test:watch": "npm run test -- --watch",
    "test": "jest --passWithNoTests --verbose",
    "ts:check": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit",
      "pre-push": "npm run pre-push"
    }
  }
}
